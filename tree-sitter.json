{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "kpl",
      "camelcase": "KPL",
      "title": "kpl",
      "scope": "source.kpl",
      "file-types": [
        "k",
        "h"
      ],
      "injection-regex": "^kpl$",
      "class-name": "TreeSitterKpl"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "KPL grammer for tree-sitter",
    "authors": [
      {
        "name": "Sean Allesina-McGrory",
        "email": "SeanAllesina@proton.me",
        "url": "https://github.com/allesis"
      }
    ],
    "links": {
      "repository": "https://github.com/allesis/tree-sitter-kpl"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}